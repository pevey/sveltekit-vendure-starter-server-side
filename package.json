{
	"name": "sveltekit-vendure-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently 'vite dev' 'yarn generate --watch'",
		"build": "shx rm -rf ./build && yarn generate && vite build",
		"preview": "vite preview",
		"test": "yarn test:integration && yarn test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"generate": "graphql-codegen"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.2.4",
		"@melt-ui/svelte": "^0.74.4",
		"@parcel/watcher": "^2.4.1",
		"@playwright/test": "^1.42.0",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.10",
		"autoprefixer": "^10.4.17",
		"concurrently": "^8.2.2",
		"dotenv": "^16.4.5",
		"formsnap": "^0.5.1",
		"graphql": "^16.8.1",
		"lucide-svelte": "^0.344.0",
		"shx": "^0.3.4",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"svelte-meta-tags": "^3.0.4",
		"sveltekit-search-params": "^2.1.1",
		"sveltekit-stripe": "^3.0.1",
		"sveltekit-superfetch": "^3.0.3",
		"sveltekit-superforms": "^2.6.2",
		"sveltekit-turnstile": "^1.2.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.3.3",
		"vite": "5.1.4",
		"vitest": "1.3.1",
		"xss": "^1.0.14",
		"zod": "^3.22.4"
	},
	"type": "module"
}
